{
  "buildCommand": "npm run build",
  "outputDirectory": "static",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api-btcfi/(.*)",
      "destination": "https://jungho.dev/api-btcfi/$1"
    },
    {
      "source": "/api-new/(.*)",
      "destination": "http://34.64.253.2:8080/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
